<?xml version="1.0" encoding="utf-8"?>
<!--<openerp>-->
<odoo>
    <data>

        <record id="account_xls_output_view_tax" model="ir.ui.view">
          <field name="name">Excel Output</field>
          <field name="model">account.xls.output.tax</field>
          <field name="arch" type="xml">
              <form string="Excel Output">
                    <separator colspan="4" string="Please save file as .xls excel format"/>
                    <field name="name" invisible="1"/>
                    <field name="xls_output" filename='name'/>
                    <footer>
                       <button string="Cancel" class="oe_link" special="cancel"/>
                   </footer>
              </form>
          </field>
        </record>
        
   </data>
<!--</openerp>-->
</odoo>
